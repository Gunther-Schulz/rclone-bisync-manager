[Unit]
Description=Rclone Bisync Wrapper Service
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
EnvironmentFile=/etc/rclone-bisync/rclone-bisync.conf
ExecStart=/usr/bin/rclone-bisync ${RCLONE_BISYNC_PATHS} ${RCLONE_BISYNC_EXTRA_OPTIONS}
User=${RCLONE_BISYNC_USER}

[Install]
WantedBy=multi-user.target